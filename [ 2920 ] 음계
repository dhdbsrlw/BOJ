#include <stdio.h>
#pragma warning(disable: 4996)

int main(void) {

	int scale[8] = {0, };
	int i;
	int det_a = 0, det_d = 0;

	for (i = 0; i < 8; i++)
		scanf("%d", &scale[i]);

	
	for (i = 0; i < 8; i++) {
		if (scale[i] == i + 1)
			det_a++;
	}


	for (i = 0; i < 8; i++) {
		if(scale[i] == 8 - i)
			det_d++;
	}


	if (det_a == 8 && scale[0] == 1)
			printf("ascending");

	else if (det_d == 8 && scale[7] == 1)
		printf("descending");

	else
		printf("mixed");


			return 0;
}
