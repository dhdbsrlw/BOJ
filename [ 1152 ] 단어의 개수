#include <stdio.h>
#pragma warning(disable: 4996)

char arr[1000000] = { 0, };
int count = 0;


int cnt() {

	for (int i = 0; arr[i] != 0; i++) {
		if (arr[i] == ' ' && arr[i + 1] != 0)
			count++;
	}

		return count;
}

int main() {

	int result;

	scanf("%[^\n]", arr); //scanf set: \n이 나타날 때까지의 값을 모두 읽어들임

    result = cnt();
    
	if (arr[0] == ' ') 	printf("%d", result);

	else printf("%d", ++result);

	return 0;
}

