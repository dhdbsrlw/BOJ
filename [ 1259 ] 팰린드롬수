#include <stdio.h>
#include <string.h>
#pragma warning(disable: 4996)

int main(void) {

	char n[100000] = {0,};
	
	for (;;) {

		int cnt = 0;

		scanf("%s", n);

		if (n[0] == '0') return 0; //(n == 0)으로 작성하면, 아스키코드 값으로 0 인식

		int len = strlen(n);

		for (int i = 0; i < (len / 2); i++) {

			if (n[0 + i] != n[len - (i + 1)]) {
				cnt++;
				break;
			}
		}

		if (cnt == 0)
			printf("yes\n");

		else
			printf("no\n");
	}
	return 0;
}

/*
(ver.2)
#include <stdio.h>
#include <math.h>
#pragma warning(disable: 4996)

int main(void) {

	int n, temp, cnt, felin;

	for (;;) { //초기화필수(for문 안의 첫번째 초기화 구간은 제일 첫번째 반복에서만 시행된다.)

		temp = 1, cnt = 0, felin = 0;

		scanf("%d", &n);

		if (n == 0) break;

		temp = n;

		while (temp > 0)
		{
			temp /= 10;
			cnt++;

		} //반복문이 끝나면 temp = 0 으로 초기화

		temp = n;

		for (; cnt > 0; cnt--) {
			felin += (temp % 10) * pow(10, cnt - 1);

			temp /= 10;
		}

		if (n == felin) printf("yes\n");
		else printf("no\n");

	}

	return 0;

}
